#include <msp430.h> 
#include <Serial_JMP.h>

/*
 * main.c
 */

//UART Interrupt Vector

void main(void) {
    WDTCTL = WDTPW | WDTHOLD;	// Stop watchdog timer

    UART_setup(9600);
	_BIS_SR(GIE);

	char buffer;
	while(1){
		UART_recive_byte(*buffer)
		UART_print(buffer);
	}
	while(!(UCA1IFG & UCTXIFG));
	UCA1TXBUF = 'T';				// Export to global variable

	while(!(UCA1IFG & UCTXIFG));
	UCA1TXBUF = 'E';				// Export to global variable

	UART_print("#o0");

	UART_print("#o1");
	UART_print("#ot");
	//while (1);

	UART_print("#ob#o1#s");

	while(1);
}
