#include <msp430.h> 
#include "Serial_JMP.h"
#include <stdlib.h>
/*
 * main.c
 */

//UART Interrupt Vector

void main(void) {
    WDTCTL = WDTPW | WDTHOLD;	// Stop watchdog timer

    USB_setup(9600);
	_BIS_SR(GIE);


	USBBuff = 'x';
	USB_receive_non_blocking();
	while(1){
		USB_transmit_byte(USBBuff);
		USBBuff = 'k';
		__delay_cycles(5000000);
	}

}
